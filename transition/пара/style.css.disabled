body {
    height: 100vh;
}


.btn{
    width: 100px;
    height: 100px;
    border-radius: 50%;
    border: none;
    background: purple;
    color: white;
    font-size: 20px;
    font-weight: 500;
    line-height: 1;
    margin: 0 auto;
    position: absolute;
    top: 50%;
    left: 50%;
    

    transition: transform 1s cubic-bezier(.77,.81,0,-1.23);
}

.btn:hover{
   
    transform: translateY(-100px);
    
}